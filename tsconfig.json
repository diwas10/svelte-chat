{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "paths": {
      "$components/*": [
        "src/components"
      ],
      "$store/*": [
        "src/store"
      ],
      "$service/*": [
        "src/service"
      ],
      "$utils/*": [
        "src/utils"
      ]
    }
  }
}
